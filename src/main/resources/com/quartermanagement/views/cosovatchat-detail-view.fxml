<AnchorPane prefHeight="600.0" prefWidth="710.0" style="-fx-background-color: transparent;" stylesheets="@../styles/nhankhau-detail.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quartermanagement.Controller.CoSoVatChat.CoSoVatChatDeTailController">
    <children>
        <!-- Title -->
        <Text fx:id="title" layoutX="228.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Thêm đồ dùng mới" textAlignment="CENTER" wrappingWidth="235.31402587890625">
            <font>
                <Font name="Segoe UI" size="18.0" />
            </font>
        </Text>
        <!--Tên đồ dùng -->
        <Text layoutX="42.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tên đồ dùng" wrappingWidth="116.5140380859375">
            <font>
                <Font name="Segoe UI" size="18.0" />
            </font>
        </Text>
        <TextField fx:id="tenDoDungTextField" layoutX="233.0" layoutY="210.0" prefHeight="30.0" prefWidth="315.0" promptText="Tên đồ dùng" style="-fx-border-color: #3f4d67; -fx-border-radius: 10px; -fx-background-color: transparent;">
            <font>
                <Font name="Segoe UI Italic" size="14.0" />
            </font>
        </TextField>
        <!-- Số lượng -->
        <Text layoutX="42.0" layoutY="286.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Số Lượng" wrappingWidth="107.7139892578125">
