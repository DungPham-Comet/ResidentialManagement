                <Font name="Segoe UI" size="18.0" />
            </font>
        </Text>
        <TextField fx:id="soLuongTextField" layoutX="233.0" layoutY="264.0" prefHeight="30.0" prefWidth="315.0" promptText="Số lượng" style="-fx-border-color: #3f4d67; -fx-border-radius: 10px; -fx-background-color: transparent;">
            <font>
                <Font name="Segoe UI Italic" size="14.0" />
            </font>
        </TextField>
        <!--Thêm mới-->
        <Button fx:id="add_btn" layoutX="406.0" layoutY="559.0" mnemonicParsing="false" onAction="#addnew" prefHeight="28.0" prefWidth="74.0" style="-fx-background-color: #3f4d67;" text="Thêm" textFill="WHITE">
            <font>
                <Font name="Segoe UI" size="14.0" />
            </font>
        </Button>
        <!--Cập nhật csvc -->
        <Button fx:id="update_btn" layoutX="509.0" layoutY="559.0" mnemonicParsing="false" onAction="#update" style="-fx-background-color: #3f4d67;" text="Cập nhật" textFill="WHITE">
            <font>
                <Font name="Segoe UI" size="14.0" />
            </font>
        </Button>
        <!--Quay lại-->
        <Button layoutX="613.0" layoutY="559.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #3f4d67;" text="Quay lại" textFill="WHITE">
            <font>
                <Font name="Segoe UI" size="14.0" />
            </font>
        </Button>
      <Pane  fx:id="maDoDungPane"    layoutX="22.0" layoutY="134.0" prefHeight="65.0" prefWidth="540.0">
         <children>
      
                    <!-- mã đồ dùng-->
              <Text layoutX="14.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mã đồ dùng" textAlignment="CENTER" wrappingWidth="107.7139892578125">
