              <TextField fx:id="soLuongKhaDungTextField" layoutX="227.0" layoutY="32.0" prefHeight="30.0" prefWidth="315.0" promptText="Số lượng khả dụng" style="-fx-border-color: #3f4d67; -fx-border-radius: 10px; -fx-background-color: transparent;">
                  <font>
                      <Font name="Segoe UI Italic" size="14.0" />
                  </font>
              </TextField>
         </children>
